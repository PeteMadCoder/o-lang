import "std/vector.olang";
import "std/io.olang";

fn exit(status: int);

fn main() -> int {
    var vec = new Vector<int>();
    
    // Test push operation
    vec.push(42);
    
    // Test size
    var size = vec.size();
    
    // Test logical NOT with boolean result from comparison
    var is_empty = vec.is_empty();  // Should be false
    var not_empty = !is_empty;      // Should be true
    
    // Print size
    print_int(size);  // Should print 1
    unsafe { printf("\n"); }
    
    // Print the result of logical NOT operation
    if (not_empty) {
        unsafe { printf("Vector is not empty (logical NOT works!)\n"); }
    }
    
    // Pop the element
    var val = vec.pop();
    print_int(val);  // Should print 42
    unsafe { printf("\n"); }
    
