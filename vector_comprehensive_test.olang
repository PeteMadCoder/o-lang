import "std/vector.olang";
import "std/io.olang";

fn exit(status: int);

fn main() -> int {
    var vec = new Vector<int>();
    
    // Test push operation
    vec.push(10);
    vec.push(20);
    vec.push(30);
    
    // Test size
    var size = vec.size();
    print_int(size);
    unsafe { printf("\n"); }
    
    // Test indexing
    var first = vec[0];
    var second = vec[1];
    var third = vec[2];
    
    print_int(first);
    unsafe { printf(" "); }
    print_int(second);
    unsafe { printf(" "); }
    print_int(third);
    unsafe { printf("\n"); }
    
    // Test pop
    var popped = vec.pop();
    print_int(popped);  // Should print 30
    unsafe { printf("\n"); }
    
    var new_size = vec.size();
    print_int(new_size);  // Should print 2
    unsafe { printf("\n"); }
    
    // Pop remaining elements
    var popped2 = vec.pop();
    var popped3 = vec.pop();
    
    print_int(popped2);  // Should print 20
    unsafe { printf(" "); }
    print_int(popped3);  // Should print 10
    unsafe { printf("\n"); }
    
    return 0;
}