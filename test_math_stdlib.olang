import "std/math.olang";
import "std/io.olang";

fn main() -> int {
    // Test integer math functions
    var abs_val = abs(-5);
    print_int(abs_val);  // Should print 5
    unsafe { printf("\n"); }
    
    var min_val = min(10, 5);
    print_int(min_val);  // Should print 5
    unsafe { printf("\n"); }
    
    var max_val = max(10, 5);
    print_int(max_val);  // Should print 10
    unsafe { printf("\n"); }
    
    var clamped = clamp(15, 1, 10);
    print_int(clamped);  // Should print 10
    unsafe { printf("\n"); }
    
    var sign_val = sign(-10);
    print_int(sign_val);  // Should print -1
    unsafe { printf("\n"); }
    
    // Test float math functions (these will call external C functions)
    var abs_f_val = abs_f(-3.14);
    // Note: print_float would be needed for floats, but we don't have that
    // Just test that the function can be called
    print_int(1);  // Placeholder to show function was called
    unsafe { printf("\n"); }
    
    var min_f_val = min_f(2.5, 1.8);
    print_int(1);  // Placeholder
    unsafe { printf("\n"); }
    
    var max_f_val = max_f(2.5, 1.8);
    print_int(1);  // Placeholder
    unsafe { printf("\n"); }
    
    var sign_f_val = sign_f(-5.7);
    print_int(-1);  // Should be -1
    unsafe { printf("\n"); }
    
    return 0;
}