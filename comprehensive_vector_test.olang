import "stdlib/std/vector.olang";
import "stdlib/std/io.olang";

fn main() -> int {
    var vec = new Vector<int>();
    
    vec.push(10);
    vec.push(20);
    vec.push(30);
    
    var first = vec[0];   // Should be 10
    var second = vec[1];  // Should be 20
    var third = vec[2];   // Should be 30
    
    var size = vec.size(); // Should be 3
    
    io.println("First: ");
    io.print_int(first);
    io.println("");
    
    io.println("Second: ");
    io.print_int(second);
    io.println("");
    
    io.println("Third: ");
    io.print_int(third);
    io.println("");
    
    io.println("Size: ");
    io.print_int(size);
    io.println("");
    
    return size;  // Return 3
}
