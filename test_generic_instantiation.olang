import "stdlib/std/vector.olang";

fn main() {
    var vec_int = new Vector<int>();
    var vec_float = new Vector<float>();
    var vec_char = new Vector<char>();
    
    // Test pushing values
    vec_int.push(42);
    vec_float.push(3.14);
    vec_char.push('A');
    
    // Test accessing values
    var val_int = vec_int[0];
    var val_float = vec_float[0];
    var val_char = vec_char[0];
    
    // Test size
    var size_int = vec_int.size();
    var size_float = vec_float.size();
    var size_char = vec_char.size();
    
    return;
}