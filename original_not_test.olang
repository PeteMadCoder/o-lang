// Test case for logical NOT operator
fn main() -> int {
    var boolean_value = true;
    var result = !boolean_value;  // This is the specific test case mentioned
    
    if (result) {
        return 1;  // Should not reach here since !true = false
    }
    
    // Also test with false
    var another_boolean = false;
    var another_result = !another_boolean;  // !false = true
    
    if (!another_result) {
        return 2;  // Should not reach here since !true = false, and !false = true
    }
    
    if (another_result) {
        return 0;  // This should execute
    }
    
    return 3;  // Should not reach here
}