// Test to isolate the generic casting issue
struct SimpleGeneric<T> {
    data: *byte;

    new() {
        this.data = 0 as *byte;  // Just initialize
    }

    fn test_cast(val: T) -> T {
        // This should trigger the same error as the vector
        unsafe {
            var ptr = this.data as *T;  // This is the problematic line
            return val;
        }
    }
}

fn main() -> int {
    var obj = new SimpleGeneric<int>();
    var result = obj.test_cast(42);
    return result;
}