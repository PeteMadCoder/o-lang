struct Point {
    int x;
    int y;
    
    new(int x_val, int y_val) {
        this.x = x_val;
        this.y = y_val;
    }
    
    fn get_x() -> int {
        return this.x;
    }
}

fn main() -> int {
    var p = new Point(10, 20);
    var x_val = p.get_x();
    return x_val;  // Should return 10
}