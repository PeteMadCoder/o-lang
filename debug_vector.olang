import "std/io.olang";
import "std/vector.olang";

fn main() -> int {
    var vec = new Vector<int>();
    
    print_int(vec.size());
    println("");  // Should print 0
    
    vec.push(42);
    
    print_int(vec.size());
    println("");  // Should print 1
    
    var val = vec[0];
    print_int(val);
    println("");  // Should print 42
    
    return 0;
}