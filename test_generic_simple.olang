import "std/io.olang";

// Simple test to see if basic generic functionality works
struct SimpleContainer<T> {
    value: T;
    
    new(val: T) {
        this.value = val;
    }
    
    fn get() -> T {
        return this.value;
    }
}

fn main() -> int {
    var container = new SimpleContainer<int>(42);
    var val = container.get();
    print_int(val);
    println("");
    return 0;
}