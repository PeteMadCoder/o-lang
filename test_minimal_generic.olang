// Simple test for generic instantiation without stdlib
struct SimpleVec<T> {
    T[4] data;
    int count;

    new() {
        this.count = 0;
    }

    fn push(T val) {
        if (this.count < 4) {
            this.data[this.count] = val;
            this.count = this.count + 1;
        }
    }
}

fn main() -> int {
    var vec_int = new SimpleVec<int>();
    var vec_float = new SimpleVec<float>();
    vec_int.push(42);
    return vec_int.count;
}