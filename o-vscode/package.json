{
  "name": "o-lang",
  "displayName": "O Language",
  "icon": "logo.png",
  "description": "Syntax highlighting for O",
  "version": "0.0.1",
  "engines": { "vscode": "^1.70.0" },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [{
      "id": "o",
      "aliases": ["O", "o-lang"],
      "extensions": [".o", ".olang"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./icons/o-file.svg",
        "dark": "./icons/o-file.svg"
      }
    }],
    "grammars": [{
      "language": "o",
      "scopeName": "source.o",
      "path": "./syntaxes/o.tmLanguage.json"
    }]
  }
}