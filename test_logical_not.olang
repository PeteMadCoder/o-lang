fn main() -> int {
    var x = true;
    var y = false;
    
    var result1 = !x;  // Should be false
    var result2 = !y;  // Should be true
    var result3 = !(1 < 2);  // Should be false
    
    if (result1) {
        return 1;
    }
    
    if (!result2) {
        return 2;
    }
    
    if (result3) {
        return 3;
    }
    
    return 0;  // Expected result
}