fn unsafeAsExpression() -> int {
    var x = 10;
    
    // Unsafe block used as an expression
    var result = unsafe {
        var temp = x * 2;
        temp + 5
    };
    
    return result; // Should be 25
}

fn main() -> int {
    var final_result = unsafeAsExpression();
    return final_result;
}
