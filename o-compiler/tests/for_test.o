fn putchar(int c) -> int;

fn print_int(int n) -> int {
    if (n < 0) {
        putchar(45); // '-'
        n = 0 - n;
    }
    if (n > 9) {
        print_int(n / 10);
    }
    var digit = n - (n / 10) * 10;
    putchar(digit + 48); // '0'
    return 0;
}

fn print_newline() -> int {
    putchar(10);
    return 0;
}

fn main() -> int {
    // Basic loop
    for (var i = 0; i < 5; i = i + 1) {
        print_int(i);
        putchar(32);
    }
    print_newline();
    
    // Empty parts (equivalent to while)
    var j = 0;
    for (; j < 3;) {
        print_int(j);
        putchar(32);
        j = j + 1;
    }
    print_newline();
    
    return 0;
}
