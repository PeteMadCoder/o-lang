open class Animal {
    new() {}
    virtual fn speak() -> int {
        return 1;
    }
}

class Dog : Animal {
    new() {}
    override fn speak() -> int {
        return 2;
    }
}

class Cat : Animal {
    new() {}
    override fn speak() -> int {
        return 3;
    }
}

fn main() -> int {
    var a = new Animal();
    var d = new Dog();
    var c = new Cat();
    
    var r1 = a.speak();
    var r2 = d.speak();
    var r3 = c.speak();
    
    return r1 + r2 + r3;
}
