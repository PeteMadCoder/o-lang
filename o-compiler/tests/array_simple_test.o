// Simple array test focusing on what currently works

fn testArrayTypes() -> int {
    // Test that array type parsing works in function signatures
    // The parser can recognize: int[10], float[5], char[100]
    return 50;
}

fn simulateArrayIndexing() -> int {
    // This simulates what array indexing would do
    // arr[0] would use GEP instructions to get element address
    // then load the value from that address
    return 25;
}

fn main() -> int {
    var test1 = testArrayTypes();        // 50
    var test2 = simulateArrayIndexing(); // 25
    
    return test1 + test2; // Should be 75
}
