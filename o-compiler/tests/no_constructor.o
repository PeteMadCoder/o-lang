struct NoConstructor {
    int x, y;
    
    fn getValue() -> int {
        return 999;
    }
}

fn testNoConstructor() -> int {
    // This should work even without a constructor
    // Just allocates memory without calling any constructor
    var obj = new NoConstructor();
    
    return 555;
}

fn main() -> int {
    var result = testNoConstructor();
    return result;
}
