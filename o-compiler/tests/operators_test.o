fn main() -> int {
    var a = 10;
    var b = 20;
    
    // Test comparison operators
    var eq = (a == 10);    // true
    var ne = (a != b);     // true  
    var lt = (a < b);      // true
    var gt = (a > b);      // false
    var le = (a <= 10);    // true
    var ge = (b >= 20);    // true
    
    // Test logical operators with short-circuiting
    var and1 = (true && true);   // true
    var and2 = (false && true);  // false (should short-circuit)
    var or1 = (true || false);   // true (should short-circuit)
    var or2 = (false || true);   // true
    
    return 0;
}
