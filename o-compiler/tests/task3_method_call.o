struct Point {
    int x;
    int y;
}

fn Point_add(*Point this, int dx, int dy) -> void {
    this.x = this.x + dx;
    this.y = this.y + dy;
}

fn Point_getX(*Point this) -> int {
    return this.x;
}

fn main() -> int {
    var p = new Point();
    p.x = 10;
    p.y = 20;
    
    // Test Method Call
    p.add(5, 5);
    
    return p.getX(); // Should be 15
}
