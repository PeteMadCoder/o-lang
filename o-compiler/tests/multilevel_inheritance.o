class Vehicle {
    int wheels;
    int speed;
    
    fn getWheels() -> int {
        return 4;
    }
}

class Car : Vehicle {
    int doors;
    int engine;
    
    fn getDoors() -> int {
        return 4;
    }
}

class SportsCar : Car {
    int turbo;
    
    fn getTurbo() -> int {
        return 1;
    }
}

fn testMultiLevelInheritance() -> int {
    var sports = new SportsCar();
    var car = new Car();
    var vehicle = new Vehicle();
    
    return 200;
}

fn main() -> int {
    var result = testMultiLevelInheritance();
    return result;
}
