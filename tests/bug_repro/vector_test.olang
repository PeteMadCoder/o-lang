import "std/vector.olang";

fn main() -> int {
    var v = new Vector<int>();
    v.push(10);
    v.push(20);
    
    // We access elements, which triggers code generation for op_index.
    // op_index calls exit(1) on failure.
    // Since we don't call exit() in main, this would have crashed the compiler before the fix.
    var val = v[0];
    
    return val;
}
