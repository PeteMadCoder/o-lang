fn main() : int {
    var t = true;
    var f = false;

    if (!t) {
        // Should not happen
        return 1;
    }

    if (!f) {
        // Should happen
    } else {
        // Should not happen
        return 2;
    }

    if (!!t) {
        // Should happen
    } else {
        return 3;
    }

    if (1 != 2) {
        // Should happen
    } else {
        return 4;
    }

    if (1 != 1) {
        return 5;
    }

    // Test with variables
    var a = 10;
    var b = 20;
    if (!(a != b)) { // !(true) -> false
        return 6;
    }
    
    if (!(a == b)) { // !(false) -> true
        // OK
    } else {
        return 7;
    }

    println("All logical NOT tests passed!");
    return 0;
}
