struct Point {
    x: int;
    y: int;
}

fn new_Point(x: int, y: int) -> Point {
    var p: Point;
    p.x = x;
    p.y = y;
    return p;
}

fn distance_squared(p: Point) -> int {
    return p.x * p.x + p.y * p.y;
}

fn main() -> int {
    var p = new_Point(3, 4);
    return distance_squared(p);
}