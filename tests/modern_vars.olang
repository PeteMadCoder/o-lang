fn main() -> int {
    // 1. Inference & Mutability
    var x = 10;
    x = 20;     // OK - mutable variable can be reassigned

    // 2. Explicit Type
    var y: int = 100;
    
    // 3. Constants
    let z = 50;
    // z = 51;  // <-- This should trigger a Compiler Error: Cannot reassign constant 'z'
    
    return x + z;
}