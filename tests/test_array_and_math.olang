import "math.olang";

fn main() -> int {
    var numbers = new int[5];  // Create a simple array
    
    // Fill the array
    numbers[0] = 1;
    numbers[1] = 2;
    numbers[2] = 3;
    numbers[3] = 4;
    numbers[4] = 5;
    
    // Use math functions to manipulate values
    var sum = 0;
    var i = 0;
    while (i < 5) {
        sum = sum + abs(numbers[i]);
        i = i + 1;
    }
    
    return sum;  // Should return 15
}