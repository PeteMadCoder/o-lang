import "std/io.olang";
import "std/vector.olang";

fn main() -> int {
    var vec = new Vector<int>();
    
    print_int(1);  // Indicator that vector creation works
    println("");
    
    vec.push(100);
    
    print_int(2);  // Indicator that push works
    println("");
    
    var sz = vec.size();
    print_int(sz);  // Should print 1
    println("");
    
    var val = vec[0];
    print_int(val);  // Should print 100
    println("");
    
    print_int(3);  // Success indicator
    println("");
    
    return 0;
}