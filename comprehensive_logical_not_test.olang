fn main() -> int {
    var x = true;
    var y = false;
    
    // Basic logical NOT
    var result1 = !x;  // Should be false
    var result2 = !y;  // Should be true
    
    // Logical NOT with comparison
    var result3 = !(1 < 2);  // Should be false (since 1 < 2 is true, !true is false)
    var result4 = !(1 > 2);  // Should be true (since 1 > 2 is false, !false is true)
    
    // Double negation
    var result5 = !!x;  // Should be true (not not true = true)
    var result6 = !!y;  // Should be false (not not false = false)
    
    // Logical NOT in conditionals
    if (!result1) {  // result1 is false, so !result1 is true
        // This should execute
    } else {
        return 1;  // Should not reach here
    }
    
    if (!result2) {  // result2 is true, so !result2 is false
        return 2;  // Should not reach here
    } else {
        // This should execute
    }
    
    if (!result3) {  // result3 is false, so !result3 is true
        // This should execute
    } else {
        return 3;  // Should not reach here
    }
    
    // Nested logical NOT in if condition
    if (!(result1 && result2)) {  // true && false = false, !false = true
        // This should execute
    } else {
        return 4;  // Should not reach here
    }
    
    return 0;  // Expected result if all tests pass
}