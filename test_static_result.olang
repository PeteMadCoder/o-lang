struct Result {
    value: int;
    success: bool;

    static fn ok(val: int) -> Result {
        var r: Result;
        r.value = val;
        r.success = true;
        return r;
    }

    fn getValue() -> int {
        return this.value;
    }
}

fn main() -> int {
    var res = Result.ok(42);
    var val = res.getValue();
    return val;
}