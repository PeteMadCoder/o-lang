// Test generic struct with constructor to trigger the instantiation issue
struct Vector<T> {
    T[10] data;
    int count;

    new() {
        this.count = 0;
    }

    fn push(T val) {
        if (this.count < 10) {
            this.data[this.count] = val;
            this.count = this.count + 1;
        }
    }
}

fn main() -> int {
    // This should trigger generic instantiation with constructors
    var vec_int = new Vector<int>();
    var vec_float = new Vector<float>();
    
    // Test with different types
    var vec_char = new Vector<char>();
    
    // Use the vectors to make sure they work
    vec_int.push(42);
    vec_float.push(3.14);
    vec_char.push('A');
    
    return vec_int.count + vec_float.count + vec_char.count;
}